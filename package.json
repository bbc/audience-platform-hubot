{
  "name": "hubot",
  "version": "0.0.0-development",
  "publishConfig": {
    "tag": "next"
  },
  "author": "hubot",
  "keywords": [
    "github",
    "hubot",
    "campfire",
    "bot"
  ],
  "description": "A simple helpful robot for your Company",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hubotio/hubot.git"
  },
  "dependencies": {
    "async": ">=0.1.0 <1.0.0",
    "chalk": "^1.0.0",
    "cline": "^0.8.2",
    "coffee-script": "1.6.3",
    "connect-multiparty": "^1.2.5",
    "cosmos-cli": "github:bbc/cosmos-cli",
    "express": "^3.21.2",
    "hubot": "^2.18.0",
    "hubot-diagnostics": "0.0.1",
    "hubot-github-repo-event-notifier": "^1.6.0",
    "hubot-help": "^0.1.3",
    "hubot-http-status": "^0.2.1",
    "hubot-maps": "0.0.2",
    "hubot-monzo-me": "^1.0.1",
    "hubot-npm": "0.0.9",
    "hubot-pugme": "^0.1.0",
    "hubot-redis-brain": "0.0.3",
    "hubot-rules": "^0.1.1",
    "hubot-seen": "^0.2.3",
    "hubot-shipit": "^0.2.0",
    "hubot-slack": "^4.4.0",
    "hubot-standup-alarm": "0.0.7",
    "hubot-thank-you": "0.0.3",
    "log": "1.4.0",
    "optparse": "1.0.4",
    "scoped-http-client": "0.11.0"
  },
  "devDependencies": {
    "body-parser": "^1.15.0",
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "eslint": "^2.8.0",
    "eslint-config-iplayer-es6": "^1.4.0",
    "eslint-plugin-mocha": "^3.0.0",
    "mocha": "^2.4.5",
    "mockery": "^1.4.0",
    "nock": "^8.0.0",
    "nyc": "^11.0.2",
    "sinon": "~1.17.4",
    "sinon-chai": "^2.8.0",
    "standard": "^10.0.2",
    "semantic-release": "^6.3.6",
    "speculate": "^1.6.0"
  },
  "engines": {
    "node": "> 4.0.0",
    "npm": "> 2.0.0"
  },
  "main": "./index",
  "bin": {
    "hubot": "./bin/hubot"
  },
  "scripts": {
    "start": "bin/hubot --name hugh -a slack",
    "pretest": "standard",
    "test": "nyc --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:smoke": "node src/**/*.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "spec": "speculate --release=${BUILD_NUMBER}",
    "lint": "eslint scripts"
  }
}
